[application]

config/name="Kavoons"
run/main_scene="res://src/scenes/SceneHandler.tscn"
config/icon="res://icon.png"

[autoload]

Constants="*res://src/singletons/Constants.gd"
Towers="*res://src/singletons/Towers.gd"
Cats="*res://src/singletons/Cats.gd"
